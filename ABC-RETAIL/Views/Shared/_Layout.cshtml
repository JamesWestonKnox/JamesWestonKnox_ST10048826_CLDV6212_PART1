
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] ABC RETAIL</title>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav>
            <ul class="navbar">
                <li>
                    <a class="" asp-area="" asp-controller="Home" asp-action="Index">ABC Retail</a>
                </li>
                <li class="">
                    <a class="" asp-area="" asp-controller="Home" asp-action="Customers">Customers</a>
                </li>
                <li class="">
                    <a class="" asp-area="" asp-controller="Home" asp-action="Products">Products</a>
                </li>
                <li class="">
                    <a class="" asp-area="" asp-controller="Home" asp-action="Orders">Orders</a>
                </li>
                <li class="">
                    <a class="" asp-area="" asp-controller="Home" asp-action="Contracts">Contracts</a>
                </li>
            </ul>
        </nav>
    </header>
    <div class="">
        <main role="main" class="">
            @RenderBody()
        </main>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
